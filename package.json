  },
  "dependencies": {
    "@fontsource/roboto": "^4.4.5",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "material-ui": "^1.0.0-beta.47",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0"
  }
}
